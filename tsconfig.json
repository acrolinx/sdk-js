{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ESNext"],
    "module": "CommonJS",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "removeComments": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "baseUrl": ".",
  },
  "include": ["src"],
  "typedocOptions": {
    "theme": "default",
    "readme": "none",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "out": "./docs/pluginDoc/"
  }
}
