{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "DOM"
    ],
    "module": "CommonJS",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "fetch-mock": ["node_modules/fetch-mock/es5/client"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "typedocOptions": {
    "theme": "default",
    "readme": "none",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "out": "./docs/pluginDoc/",
  }
}
