{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ES2015.Promise",
      "ES2015.Iterable",
      "ES5",
      "DOM"
    ],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "fetch-mock": ["node_modules/fetch-mock/es5/client"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
